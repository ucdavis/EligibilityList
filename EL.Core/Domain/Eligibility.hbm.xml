<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">

  <class name="EL.Core.Domain.Eligibility, EL.Core" table="EligibilityList">
    <id name="ID" access="nosetter.lowercase" column="AppointmentID" unsaved-value="0">
      <generator class="identity" />
    </id>
    
    <!--<property name="DeptCode" column="DIST_DEPT_CODE" />-->    
    <property name="DeanID" column="DADAssociateDeanID" />
    <property name="AnalystID" column="AnalystID" />
    <!--<property name="ActionID" column="ActionID" />-->
    <property name="YearsAtRank" column="YearsAtRank" />
    <property name="YearsAtStep" column="YearsAtStep" />
    <property name="AppointmentPercent" column="AppointmentPercent" />
    <property name="CommitteeID" column="CommitteeID" />
    <!--<property name="TitleCode" column="TITLE_CODE" />-->
    <!--<property name="CurrentStep" column="CurrentStep" />-->
    <property name="CurrentBlankTitle" column="CurrentBlankTitle" />
    <!--<property name="ProposedTitleCode" column="ProposedTitleCode" />-->
    <!--<property name="ProposedStep" column="ProposedStep" />-->
    <property name="ProposedBlankTitle" column="ProposedBlankTitle" />
    <property name="YearsAccelerated" column="YearsAccelerated" />
    <property name="YearsDecelerated" column="YearsDecelerated" />
    <property name="Defer" column="Defer" />
    <property name="DateDue" column="DateDue" />
    <property name="DateToCommittee" column="DateToCommittee" />
    <property name="DateCommitteeReceived" column="DateCommitteeReceived" />
    <property name="DateEffective" column="DateEffective" />
    <property name="Comment" column="Comment" />
    <property name="FinalAction" column="FinalAction" />
    <property name="TPCCode" column="TPC_PAY_REP_CODE" />
    <property name="AppNum" column="APPT_NUM" />
    <property name="LastUpdated" column="LastUpdated" />
    <property name="ProposedAppointmentPercent" column="ProposedAppointmentPercent" />
    <property name="IsActive" column="IsActive" />
    
    <!--///TODO: Ask Adam if OrigEL needs to be not null-->
    <many-to-one name="OriginalEligibility" column="OriginalApptID" class="EL.Core.Domain.Eligibility, EL.Core" not-null="false"></many-to-one>
    <many-to-one name="Employee" column="EmployeeID" class="EL.Core.Domain.Employee, EL.Core" not-null="true"></many-to-one>
    <many-to-one name="Department" column="DIST_DEPT_CODE" class="EL.Core.Domain.Department, EL.Core" not-null="true"></many-to-one>
    <many-to-one name="Action" column="ActionID" class="EL.Core.Domain.Action, EL.Core" not-null="true"></many-to-one>
    <many-to-one name="CurrentStep" column="CurrentStep" class="EL.Core.Domain.Step, EL.Core" not-null="true"></many-to-one>
    <many-to-one name="ProposedStep" column="ProposedStep" class="EL.Core.Domain.Step, EL.Core"></many-to-one>
    <many-to-one name="CurrentTitle" column="TITLE_CODE" class="EL.Core.Domain.Title, EL.Core" not-null="true"></many-to-one>
    <many-to-one name="ProposedTitle" column="ProposedTitleCode" class="EL.Core.Domain.Title, EL.Core" not-null="false"></many-to-one>


  </class>

</hibernate-mapping>
